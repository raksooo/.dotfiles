#!/usr/bin/env sh

if [[ "graphics" == $1* ]]; then
    URL="http://www.cse.chalmers.se/edu/course/TDA361/"
elif [[ "math" == $1* ]]; then
    URL="http://www.math.chalmers.se/Stat/Grundutb/CTH/mve051/1617/"
elif [[ "datavetaren" == $1* ]]; then
    URL="http://www.cse.chalmers.se/edu/course/DAT315/"
fi

chromium "$URL" 2<&1 &> /dev/null

